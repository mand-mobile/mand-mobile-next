.md-notice-bar
  --md-notice-bar-fill: rgba(89, 158, 248, .08)
  --md-notice-bar-font-size: var(--md-font-body-normal)
  --md-notice-bar-color: var(--md-color-primary)
  --md-notice-bar-border-radius: 32px
  --md-notice-bar-fill-warning: #FFEEEF
  --md-notice-bar-color-warning: #FF5B60
  --md-notice-bar-fill-activity: #FFEDDE
  --md-notice-bar-color-activity: #FF843D
  --md-notice-bar-zindex: 999